{
  "name": "kronos-interceptor-http-request",
  "version": "0.0.0-semantic-release",
  "description": "Interceptor to convert messages into multipart form data and back",
  "keywords": [
    "kronos-interceptor-http-request",
    "kronos-interceptor"
  ],
  "main": "dist/module.js",
  "module": "src/module.js",
  "scripts": {
    "cover": "nyc npm test",
    "doc": "./node_modules/.bin/jsdoc lib/*.js",
    "test": "mocha tests/*-test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "rollup -c",
    "posttest": "markdown-doctest",
    "precover": "rollup -c",
    "prepare": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-interceptor-http-request.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-http-request/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-interceptor-http-request#readme",
  "engines": {
    "node": ">=8.8.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "jsdoc-to-markdown": "^3.0.0",
    "kronos-service-manager": "^3.5.10",
    "kronos-test-interceptor": "^2.1.0",
    "markdown-doctest": "^0.9.1",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "rollup": "^0.50.0",
    "semantic-release": "^8.2.2"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "dependencies": {
    "busboy": "^0.2.14",
    "expression-expander": "^6.0.0",
    "kronos-interceptor": "^2.5.0",
    "model-attributes": "^3.0.10"
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}
