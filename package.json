{
  "name": "kronos-interceptor-http-request",
  "version": "0.0.0-semantic-release",
  "description": "Interceptor to convert messages into multipart form data and back",
  "keywords": [
    "kronos-interceptor-http-request",
    "kronos-interceptor"
  ],
  "main": "dist/module.js",
  "module": "src/module.js",
  "scripts": {
    "cover": "nyc npm test",
    "doc": "./node_modules/.bin/jsdoc lib/*.js",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=dist/module.js -c -- src/module.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-http-request.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-http-request/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-interceptor-http-request#readme",
  "engines": {
    "node": ">=7.9.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "kronos-service-manager": "^3.5.10",
    "kronos-test-interceptor": "^2.1.0",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.3.0",
    "nyc": "^10.2.0",
    "rollup": "^0.41.6",
    "semantic-release": "^6.3.6"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "dependencies": {
    "busboy": "^0.2.14",
    "expression-expander": "^5.3.10",
    "kronos-interceptor": "^2.4.16",
    "model-attributes": "^3.0.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  }
}